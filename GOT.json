const { rejects } = require("assert");
const fs = require("fs");

function fetchres(index) {
  return new Promise((resolve, reject) => {
    fetch("https://www.boredapi.com/api/activity")
      .then((response) => response.json())
      .then((result) => {
        const active = result.activity;

        fs.writeFile(zAnswer${index}.txt, active, (err) => {
          index++;
          if (err) {
            console.log(err);
          }
        });
        resolve(active);
      })
      .catch((error) => {
        console.error(error);
        reject(error);
      });
  });
}

/*
for (let index = 0; index < 3; index++) {
  fetchres(index);
}
*/
Promise.all([fetchres(1), fetchres(2), fetchres(3)]).then((result) => {
  console.log(result);
});
